<script type="text/javascript">
//定义标签的初始颜色
var initialColor='#008AE4';
var initialBackgroundColor='#FFFFFF';

function showSubMenu(){
	var objMainMenuDiv=document.getElementById('topMenuUl');
	var aObj=objMainMenuDiv.getElementsByTagName("a");
	for(var i=0;i<aObj.length;i++){
		if(aObj[i].getAttribute("className")=="subMenu"){
			//以a为中心,取a的外层div，通过外层div，取得a下面的内层div
			var outerLiObj=aObj[i].parentNode;
			var innerUlObj=outerLiObj.getElementsByTagName("ul");
			if(innerUlObj.length<1){
				continue;
			}
			outerLiObj.onmouseover=function(){
				var innerUlObj=this.getElementsByTagName("ul");
				innerUlObj[0].style.display="block";
			}
			outerLiObj.onmouseout=function(){
				var innerUlObj=this.getElementsByTagName("ul");
				innerUlObj[0].style.display="none";
			}
		}
	}
}
showSubMenu();
function showCategory(section,id){
	var objSection=document.getElementById(section);
	//取这个块下的所有div,其中第一个块是菜单，余下的是新闻列表块
	var aObj=objSection.getElementsByTagName("div");
	//当前块的菜单
	var aObjMenu=aObj[0].getElementsByTagName("a");
	//处理菜单颜色
	for(var i=0;i<aObjMenu.length;i++){
		aObjMenu[i].style.backgroundColor=initialBackgroundColor;
	}
	aObjMenu[id-1].style.backgroundColor=initialColor;
	//处理新闻标题块
	for(var i=1;i<aObj.length;i++){
		aObj[i].style.display='none';
	}
	aObj[id].style.display='block';
}
//初始化一些选项卡标签的颜色
function initMenuItem(){
	var aObj=document.getElementsByTagName("a");
	for(var i=0;i<aObj.length;i++){
		if(aObj[i].getAttribute("className")=="initialMenuItem"){
			//以a为中心,取a的外层div，通过外层div，取得a下面的内层div
			aObj[i].style.backgroundColor=initialColor;
		}
	}	
}
initMenuItem();
</script>
